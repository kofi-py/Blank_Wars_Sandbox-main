  active_challenges [label=<
    <TABLE BORDER="2" CELLBORDER="0" CELLSPACING="0" BGCOLOR="white">
      <TR><TD BGCOLOR="#3498DB" ALIGN="CENTER"><FONT COLOR="white" POINT-SIZE="14"><B>active_challenges</B></FONT></TD></TR>
      <TR><TD ALIGN="LEFT" PORT="id">id ðŸ”‘ : uuid</TD></TR>
